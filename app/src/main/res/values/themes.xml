<resources>

    <style name="Theme.DietiEstates25" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.MyApp.Home" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="homeNavbarState">@drawable/button_state</item>
        <item name="summaryNavbarState">@null</item>
        <item name="profileNavbarState">@null</item>
    </style>

    <style name="Theme.MyApp.Summary" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="homeNavbarState">@null</item>
        <item name="summaryNavbarState">@drawable/button_state</item>
        <item name="profileNavbarState">@null</item>
    </style>

    <style name="Theme.MyApp.Profile" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="homeNavbarState">@null</item>
        <item name="summaryNavbarState">@null</item>
        <item name="profileNavbarState">@drawable/button_state</item>
    </style>
</resources>